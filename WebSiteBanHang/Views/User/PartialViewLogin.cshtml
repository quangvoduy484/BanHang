<div class="modal-dialog ">
    <div class="modal-content">
        <div class="modal-header " style="background:#FFCC66; border: 1px solid rgba(0,0,0,.125);border-radius: .25rem;">
            <button data-dismiss="modal" class="close"><span>&times;</span></button>
            <div class="modal-title">Đăng nhập nhà xinh</div>
        </div>
        <div class="modal-body form-horizontal">
            <div id="loaderror" style="padding:10px;position:relative;bottom:10px;"></div>
            <div class="form-group">
                <label class="control-label col-md-3">Email hoặc số điện thoại*:</label>
                <div class="col-md-8">
                    <input class="form-control" id="EmailorPhone" name="EmailorPhone" placeholder="Email" />

                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">Mật khẩu*:</label>
                <div class="col-md-8">
                    <input class="form-control" id="Password" placeholder="Mật khẩu" name="Password" type="password" />
                </div>
            </div>

        </div>
        <div class="modal-footer">
            <button  id="btnLogin" class="btn btn-success">Đăng nhập</button>
            <button class="btn btn-default" data-dismiss="modal">Hủy</button>
        </div>
    </div>

</div>

